"use strict";angular.module("angularspaApp",["ngAnimate","ngCookies","ngResource","ngRoute","ngSanitize","ngTouch"]).config(function(a){a.when("/",{templateUrl:"views/main.html",controller:"MainCtrl",controllerAs:"main"}).when("/about",{templateUrl:"views/about.html",controller:"MainCtrl",controllerAs:"about"}).otherwise({redirectTo:"/"})}),angular.module("angularspaApp").controller("MainCtrl",["$scope","Users",function(a,b){this.awesomeThings=["HTML5 Boilerplate","AngularJS","Karma"],this.userData={name:"",surname:"",address:"",mobile:"",email:""},this.reset=function(){this.userData.name="",this.userData.surname="",this.userData.address="",this.userData.mobile="",this.userData.email=""},this.list=function(){return b.userList},this.add=function(){b.add(JSON.parse(JSON.stringify(this.userData))),this.reset()},this.remove=function(a){b.remove(a)}}]),angular.module("angularspaApp").directive("ngkFocus",function(){return{link:function(a,b,c,d){b.focus()}}}),angular.module("angularspaApp").factory("Users",function(){var a=[],b={};return b.userList=a,b.add=function(a){this.userList.push(JSON.parse(JSON.stringify(a)))},b.remove=function(a){this.userList.splice(a,1)},b});